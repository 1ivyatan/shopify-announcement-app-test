<script>
    window.TSUFFIXSendLog = function ({ level, type, message, properties, offerId, offerType }) {
        fetch("/apps/libautech-upsell/logs", {
            method: "POST",
            headers: {
                "Content-Type": "application/json"
            },
            body: JSON.stringify({
                level: level,
                type: type,
                message: message,
                pageUrl: window.location.href,
                offerId: offerId,
                offerType: offerType,
                properties: properties
            })
        });
    };
</script>